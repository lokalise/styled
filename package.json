{
	"name": "@lokalise/styled",
	"version": "2.1.0",
	"scripts": {
		"build": "vite build",
		"lint:eslint": "eslint --cache . --ext .js,.cjs,.ts",
		"lint:ts": "tsc --noEmit",
		"lint:prettier": "prettier --check .",
		"lint": "npm run lint:eslint && npm run lint:ts && npm run lint:prettier",
		"lint:fix": "npm run lint:eslint -- --fix && prettier --write .",
		"test": "vitest run",
		"prepare": "husky install",
		"commit": "commit"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./*": {
			"import": "./dist/*.js",
			"require": "./dist/*.cjs"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"types": "./dist/index.d.ts",
	"dependencies": {
		"@types/lodash-es": "^4.17.8",
		"@types/styled-components": "^5.1.26",
		"jest-styled-components": "^7.1.1",
		"lodash-es": "^4.17.21",
		"styled-components": "^5.3.11"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.7",
		"@commitlint/config-conventional": "^17.6.7",
		"@commitlint/prompt-cli": "^17.6.7",
		"@lokalise/eslint-config-frontend": "^3.0.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^10.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.0.3",
		"@semantic-release/npm": "^10.0.4",
		"@semantic-release/release-notes-generator": "^11.0.4",
		"eslint": "^8.46.0",
		"expect-type": "^0.16.0",
		"husky": "^8.0.3",
		"prettier": "^3.0.1",
		"semantic-release": "^21.0.7",
		"typescript": "~5.1.6",
		"vite": "^4.4.9",
		"vite-plugin-dts": "^3.5.1",
		"vitest": "^0.34.1"
	}
}
